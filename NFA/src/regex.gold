	Q:= {"A", "B", "C", "D", "E"}
	Σ:= {'a','b'}
	q₀:= "E"
	F:={"C"}
	Δ:=⟦⟦"A",'λ',"C"⟧,⟦"A",'a',"D"⟧,⟦"A",'b',"B"⟧,
		⟦"B",'λ',"C"⟧,
		⟦"D",'λ',"C"⟧,
