 using gold.structures.automaton.*
using gold.visualization.automaton.*

procedure main(args:String[]) begin
	Q := {"A", "B"}
	Σ := ('a'‥'d')
	q := "A"
	F := {"A"}
	Δ := ⟦⟦"A", 'b', "B"⟧,
	⟦"B", 'b', "A"⟧,
	⟦"A", 'c', "A"⟧,
	⟦"A", 'a', "A"⟧,
	⟦"A", 'd', "A"⟧⟧
	A : GNondeterministicAutomaton := GNondeterministicAutomaton(Q, Σ, q, F, Δ)
	GAutomataFrame.show(A)
end

